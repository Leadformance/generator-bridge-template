{
  //  Enforcing options
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "freeze": true,
  "immed": true,
  "indent": 4,
  "latedef": "nofunc",
  "maxcomplexity": 8,
  "maxdepth": 4,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": true,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  // Relaxing options
  "eqnull": true,
  "esnext": true,
  "globalstrict": true,
  // Environments
  "browser": true,
  "jquery": true,
  "globals": {
    "_gaq": true,
    "_gat": true,
    "moment": true
  }
}
